var registerDoubleKeyHandler = function(registerKeyCode, eventName, callback)
{
  registerKeyHandler(registerKeyCode, eventName, function(event) {
    if (this.tpShortcutTimeout && this.tpPrevKey == registerKeyCode)
    {
      clearTimeout(this.tpShortcutTimeout);
      this.tpShortcutTimeout = null;
      
      return callback(event);
