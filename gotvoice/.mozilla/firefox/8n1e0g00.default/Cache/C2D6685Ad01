<script>
//Copyright 2003-2007 A9.com or its affiliates
a9AdsViewNamespace = new Object();
a9AdsViewNamespace.buildUI=function()
{
    a9AdsViewNamespace.adsUIBlock=document.getElementById("A9AdsBottomBoxTop");
    a9AdsViewNamespace.viewServer=document.location.protocol + "//" + "";
    if( a9AdsViewNamespace.adsUIBlock )
    {
        // Add a iframe that allows us to ping the server when this element becomes visible
        a9AdsViewNamespace.idList = ['g.pMJRTHmZ1PlUt1ECck90nWDaE_', 'AOCzC.-AQM0tNmNkhMDc2Bu8y9U_', 'GJ-d5oqngi6a3zOEReCpt8D1Gm8_'];
        a9AdsViewNamespace.checking=null;
        a9AdsViewNamespace.setupAdLookingEvents();
        a9AdsViewNamespace.scheduleVisibleCheck();
        a9AdsViewNamespace.dataSent = false;
        a9AdsViewNamespace.document = document;
    }
}

a9AdsViewNamespace.setupAdLookingEvents=function()
{
    if(window.addEventListener)
    {
        window.addEventListener('resize', a9AdsViewNamespace.scheduleVisibleCheck, false);
        window.addEventListener('scroll', a9AdsViewNamespace.scheduleVisibleCheck, false);
    }
    else if(window.attachEvent)
    {
        window.attachEvent('onresize', a9AdsViewNamespace.scheduleVisibleCheck);
        window.attachEvent('onscroll', a9AdsViewNamespace.scheduleVisibleCheck);
    }
}

a9AdsViewNamespace.removeAdLookingEvents=function()
{
    if(window.removeEventListener)
    {
        window.removeEventListener('resize', a9AdsViewNamespace.scheduleVisibleCheck, false);
        window.removeEventListener('scroll', a9AdsViewNamespace.scheduleVisibleCheck, false);
    }
    else if(window.detachEvent)
    {
        window.detachEvent('onresize', a9AdsViewNamespace.scheduleVisibleCheck);
        window.detachEvent('onscroll', a9AdsViewNamespace.scheduleVisibleCheck);
    }
}

a9AdsViewNamespace.areAdsVisible=function(uiElem)
{
    if (uiElem == null)
        return false;
    var scrollTop = document.body.scrollTop;
    var scrollBottom = scrollTop + document.body.clientHeight;

    var adsTop = uiElem.offsetTop;
    var adsBottom = adsTop + uiElem.scrollHeight;

    return (adsTop <= scrollBottom && scrollTop <= adsBottom);
}

a9AdsViewNamespace.checkAdsViewable=function()
{
    if(a9AdsViewNamespace.dataSent == true)
        return;

    var uiElem = a9AdsViewNamespace.adsUIBlock;

    if(a9AdsViewNamespace.areAdsVisible(uiElem))
    {
        if(a9AdsViewNamespace.dataSent != true)
        {
            a9AdsViewNamespace.dataSent = true;
            setTimeout(a9AdsViewNamespace.checkAdsStillVisible, 4000);
        }
    }
}

a9AdsViewNamespace.checkAdsStillVisible = function()
{
    if(a9AdsViewNamespace.areAdsVisible(a9AdsViewNamespace.adsUIBlock))
    {
        var src = a9AdsViewNamespace.viewServer + '?';
        for(var i=0; i<a9AdsViewNamespace.idList.length; i++)
        {
            src = src + 'view_id=' + a9AdsViewNamespace.idList[i] + '&';
        }
        src = src + 'rand=' + Math.random();
        var scriptElement=document.createElement("script");
        scriptElement.id="a9AdsViewScriptElement";
        scriptElement.type = "text/javascript";
        scriptElement.language = "javascript";
        scriptElement.src=src;
        a9AdsViewNamespace.document.body.insertBefore(scriptElement, document.body.firstChild);
        a9AdsViewNamespace.removeAdLookingEvents();
    }
    else
    {
        a9AdsViewNamespace.dataSent = false;
    }
}

a9AdsViewNamespace.scheduleVisibleCheck=function()
{
    if(a9AdsViewNamespace.dataSent == true)
        return;

    if( a9AdsViewNamespace.checking )
    {
        clearTimeout(a9AdsViewNamespace.checking);
        a9AdsViewNamespace.checking=null;
    }
    a9AdsViewNamespace.checking=setTimeout(a9AdsViewNamespace.checkAdsViewable, 100);
}

a9AdsViewNamespace.buildUI();
</script>





