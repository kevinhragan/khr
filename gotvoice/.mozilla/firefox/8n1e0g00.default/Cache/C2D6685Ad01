{
  var link = document.getElementById(linkId);
  if (link)
  {
    document.location = link.href;
  }
};


registerDoubleKeyHandler('80', 'keyup', function() { tpFollowLink('pagnPrevLink') }); 
registerDoubleKeyHandler('78', 'keyup', function() { tpFollowLink('pagnNextLink') }); 


var fullResultScroll = function(event)
{
  
  if (event.shiftKey)
  {
    return true;
  }
  
  var pagetop = goN2U.getScrollTop();
  var pagebottom = pagetop + goN2U.getInsideWindowHeight();

  
  var i;
  for (i = 0; i < 5; i++)
  {
    var item = document.getElementById('search:Td:' + i);
    if (item)
    {
      var top = goN2U.getPageElementTop(item);
      var bottom = top + goN2U.getElementHeight(item);
      
      
       
      
      if (bottom > pagebottom &&
          top > pagetop) 
      {
        goN2U.animateScrollTo(top, 25, 20, 25);
        return false;
      }
    }
  }
  
  return true;
};


registerKeyHandler('32', 'keydown', fullResultScroll);

