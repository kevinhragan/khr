					document.write('<p class="sponsor_text">' +  google_ads[i].line2 + ' ' + google_ads[i].line3 +  '</p>');
					document.write('<p class="sponsor_footer"><a href="' +  google_ads[i].url + '" onMouseOver="return GOOGLEShowStatusInfo(\'' + escape(google_ads[i].visible_url) + '\')">' + google_ads[i].visible_url +  '</a></p>');
			document.write('</li>');				
		}
	
		document.write('</ul>');
		document.write('<p class="right_text"><a href="http://www.google.com/ads_by_google.html"> Ads by Google </a></p>');				
		document.write('</div>');
	}
}	
// -->
</script>


		
