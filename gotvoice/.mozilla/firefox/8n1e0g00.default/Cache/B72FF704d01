	var imgSrcPrefix = "http://icons-pe.wxug.com/graphics/";

	var stationselector = $('stationselector'), curcondload = $('curcondload'), condsFader = new Fx.Style('curcondbox', 'opacity' ,{duration: 400});
	stationselector.addEvent('change', function(e){
		e = new Event(e).stop();
		var x = stationselector.selectedIndex;
		var value = document.getElementsByTagName("option")[x].value;
		new Ajax(value, { method: 'get', onComplete: function(){
			curcondload.setStyle('background', 'url('+imgSrcPrefix+'arrow-loading-white.gif) no-repeat center;');
			condsFader.start(1,0).chain(function(){
				new Ajax(condsboxurl, { method: 'get', update: 'curcondbox', evalScripts: 'yes', onComplete: function(){
						condsFader.start(0,1).chain(function(){
							curcondload.setStyle('background','');
						});
					}
				}).request();
			});
			}
		}).request();
	});
});
