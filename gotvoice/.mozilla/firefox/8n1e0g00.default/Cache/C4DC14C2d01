							function enterSearch(e) {
								if (!e) e = window.event;
								if (e.type == 'keypress' && e.keyCode != '13') {
									// enter-key executes search
									return;
								}
								checkSearch();
							}
								
							function checkSearch() {
								if ((document.searchForm.queryText1.value=="") ||
									(document.searchForm.queryText1.value == "Enter Search Query")) {
									alert("Please Enter Some Search Text");
									return false;
								}	
								else {
									document.searchForm.go_search.value = "Searching...";
									document.searchForm.go_search.disabled = true;
									document.searchForm.submit();	
								}
							}
							
							function getSearchFocus() {
								if (document.searchForm.queryText1.value == "Enter Search Query")
									document.searchForm.queryText1.value = "";
								document.searchForm.queryText1.onkeypress = enterSearch;
							}
						
							function looseSearchFocus() {
								if (document.searchForm.queryText1.value == "")
									document.searchForm.queryText1.value = "Enter Search Query";
								document.searchForm.queryText1.onkeypress = null;
							}
